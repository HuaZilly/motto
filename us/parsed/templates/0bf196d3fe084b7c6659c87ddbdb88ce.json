{"components/common/wishlist-dropdown":"<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\n\n    {{#if customer.wishlists}}\n        {{#each (limit customer.wishlists 1)}}\n            <div>\n                <input class=\"btn btn-dark btn--wishlist {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\"\n                        formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                        type=\"submit\"\n                        value=\"{{this.name}}\"\n                >\n            </div>\n        {{/each}}\n    {{else}}\n        <div>\n            <input class=\"btn btn-dark btn--wishlist\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        </div>\n    {{/if}}\n</form>\n"}
