{"components/common/wishlist-dropdown":"<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\n\n    {{#if customer.wishlists}}\n        {{#each (limit customer.wishlists 1)}}\n            <div>\n                <input class=\"btn btn-dark btn--wishlist {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\"\n                        formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                        type=\"submit\"\n                        value=\"{{this.name}}\"\n                >\n            </div>\n        {{/each}}\n    {{else}}\n        <div>\n            <input class=\"btn btn-dark btn--wishlist\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        </div>\n    {{/if}}\n</form>\n","components/products/__floating-addtocart":"<div class=\"floating-addtocart d-none\">\n  <section class=\"productView-details product-options\">\n    <div class=\"productView-options position-relative\">\n        <form id=\"addToCart-form-floating\">\n        <!-- prettier-ignore -->\n        {{#each product.options}}\n        <select name=\"options-floating\" required data-product-option-change-floating>\n          <option value=\"\">Select a size</option>\n          {{#each this.values}}\n          <option value=\"{{id}}\">{{this.label}}</option>\n          {{/each}}\n        </select>\n        {{/each}}\n\n        <div class=\"form-action form-action--addtocart\" role=\"status\" aria-live=\"polite\">\n          <!-- prettier-ignore -->\n            <input \n                {{#unless product.can_purchase}} disabled=\"disabled\"{{/unless}} \n                id=\"addToCart-floating\" \n                data-wait-message=\"{{lang 'products.adding_to_cart'}}\" \n                class=\"btn btn-dark btn--addtocart mb-0 w-100\" \n                type=\"submit\" \n                value=\"{{#if product.pre_order}}{{lang 'products.pre_order'}}{{else}}{{lang 'products.add_to_cart'}}{{/if}}\" \n            />\n        </div>\n\n    </form>\n\n      {{#if settings.show_wishlist}} {{> components/common/wishlist-dropdown}} {{/if}}\n    </div>\n  </section>\n</div>\n"}
