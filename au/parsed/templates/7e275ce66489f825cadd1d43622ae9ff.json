{"components/common/modal/modal-close-btn-search":"<div class=\"header__search close\">\n  <button class=\" navUser-action navUser-action--quickSearch navUser-action--closesearch\"\n  type=\"button\"\n  title=\"{{lang 'common.close'}}\"\n  {{additional_attr}}\n>\n<span class=\"aria-description--hidden\">{{lang 'common.close'}}</span>\n<span aria-hidden=\"true\">&#215;</span>\n</button>\n</div>\n\n","components/common/quick-search-bkup-aug-17-2021":"<div class=\"form-searchbox\">\n    {{> components/common/modal/modal-close-btn-search additional_attr='data-drop-down-close' }}\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n    <div class=\"form-searchbox__top\">\n        <div class=\"container\">\n           \n                <fieldset class=\"form-fieldset form-fieldset--formsearch\">\n                    <div class=\"form-field form-field--group\">\n                        <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                        <input class=\"form-input\"\n                               data-search-quick\n                               name=\"{{name}}\"\n                               id=\"{{name}}\"\n                               data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                               placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                               autocomplete=\"off\"\n                        >\n                        <button type=\"submit\" class=\"form-search d-flex justify-content-center align-items-center\">\n                            <i class=\"icon icon--searchnav\">\n                                <svg>\n                                    <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-search\"></use>\n                                </svg>  \n                            </i>\n                        </button>\n                    </div>\n                </fieldset>\n          \n               \n        </div>\n    </div>\n    <div class=\"form-searchbox__btm\">\n        <div id=\"loaderimageQuickSearch\" class=\"loadingOverlay loadingOverlay--quicksearch\" style=\"display: none;\"></div>\n        <div class=\"container\">\n            <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n            <p role=\"status\"\n               aria-live=\"polite\"\n               class=\"aria-description--hidden\"\n               data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n            ></p>\n        </div>\n    </div>\n</form>\n</div>\n\n"}
