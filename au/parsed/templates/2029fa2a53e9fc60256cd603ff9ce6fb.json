{"components/account/wishlist-list":"<table class=\"table table--line wishlists-table my-0\">\n  <thead class=\"table-thead\">\n    <tr>\n      <th>{{lang 'account.wishlists.name'}}</th>\n      <th>{{lang 'account.wishlists.num_items'}}</th>\n      <th>{{lang 'account.wishlists.shared'}}</th>\n      <th>{{lang 'account.wishlists.action'}}</th>\n    </tr>\n  </thead>\n  <tbody class=\"table-tbody\">\n    {{#each customer.wishlists}}\n    <tr>\n      <td><a href=\"{{view_url}}\">{{{name}}}</a></td>\n      <td>{{num_items}}</td>\n      <td>{{#if is_public }} {{lang 'common.yes'}} {{else}} {{lang 'common.no'}} {{/if}}</td>\n      <td class=\"table-actions\">\n        <form method=\"post\" action=\"{{delete_url}}\" class=\"form\">\n          <fieldset class=\"form-fieldset\">\n            {{#if is_public}}\n            <a href=\"{{share_url}}\" class=\"btn btn-primary btn-sm\" role=\"button\">{{lang 'common.share'}}</a>\n            {{/if}}\n            <a href=\"{{edit_url}}\" class=\"btn btn-primary btn-sm\" data-wishlist>{{lang 'common.edit'}}</a>\n            <input type=\"submit\" value=\"{{lang 'common.delete'}}\" class=\"btn btn-primary btn-sm\" data-wishlist-delete />\n          </fieldset>\n        </form>\n      </td>\n    </tr>\n    {{/each}}\n  </tbody>\n</table>\n<form method=\"post\" action=\"{{this.urls.account.wishlists.delete}}\" class=\"form\">\n  {{#unless customer.wishlists}}\n  <p class=\"form-actions mt-5\">{{> components/common/alert/alert-info (lang 'account.wishlists.you_have_none')}}</p>\n\n  {{/unless}}\n  <div class=\"form-actions mt-5\">\n    <a href=\"{{this.urls.account.wishlists.add}}\" class=\"btn btn-primary w-auto\" role=\"button\" data-wishlist>{{lang 'account.wishlists.new'}}</a>\n    {{inject 'wishlistDelete' (lang 'account.wishlists.delete_alert')}} {{#if customer.wishlists}} {{#each customer.wishlists}}\n    <input type=\"hidden\" value=\"{{this.id}}\" name=\"deletewishlist[]\" />\n    {{/each}}\n    <input type=\"submit\" value=\"{{lang 'account.wishlists.delete_all'}}\" class=\"btn btn-outline-dark w-auto\" data-wishlist-delete />\n    {{/if}}\n  </div>\n</form>\n","components/common/alert/alert-info":"<div class=\"alertBox alertBox--info\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon bb\" aria-hidden=\"true\" ><svg width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" preserveAspectRatio=\"xMaxYMid meet\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n"}
