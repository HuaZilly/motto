{"components/faceted-search/faceted-search-navigation-bkup-april-12-2023":"<div class=\"accordion-nav-clear-holder\">\n  <button\n    type=\"button\"\n    class=\"accordion-navigation toggleLink  rounded-0 p-0  {{#unless start_collapsed}} is-open{{/unless}}\"\n    data-collapsible=\"#facetedSearch-content--{{dashcase facet}}\"\n    aria-label=\"{{lang 'search.faceted.toggleSection' title=title}}\"\n  >\n    <span class=\"accordion-title\"> {{title}} </span>\n\n    <span class=\"accordion-indicator\">\n      <svg\n        class=\"icon accordion-indicator toggleLink-text toggleLink-text--off\"\n      >\n        <use xlink:href=\"#icon-add\" />\n      </svg>\n      <svg class=\"icon accordion-indicator toggleLink-text toggleLink-text--on\">\n        <use xlink:href=\"#icon-remove\" />\n      </svg>\n    </span>\n  </button>\n  {{#if selected}}\n  <a\n    href=\"{{remove_url}}\"\n    class=\"facetedSearch-clearLink\"\n    data-faceted-search-facet\n    >{{lang 'search.faceted.clear'}}</a\n  >\n  {{/if}}\n</div>\n"}
