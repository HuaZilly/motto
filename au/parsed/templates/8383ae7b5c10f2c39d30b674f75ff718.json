{"components/common/share-product-dtl":"{{assignVar 'encodedUrl' (encodeURI product.url)}} {{assignVar 'encodedTitle'\n(encodeURI product.title)}}\n\n<ul class=\"socialLinks socialLinks--product-dtl\">\n  <li class=\"socialLinks-item socialLinks-item--facebook\">\n    <a\n      class=\"addthis_button_facebook socialLinks__link socialLinks--link icon icon--facebook\"\n      title=\"{{{capitalize service}}}\"\n      href=\"https://facebook.com/sharer/sharer.php?u={{getVar 'encodedUrl'}}\"\n      target=\"_blank\"\n      rel=\"noopener\"\n      {{{annotation}}}\n    >\n      <i class=\"icon icon-socialLinks\">\n        <svg>\n          <use xlink:href=\"#icon-facebook\"></use>\n        </svg>\n      </i>\n      <span class=\"socialLinks-title\">Facebook</span>\n    </a>\n  </li>\n  <li class=\"socialLinks-item socialLinks-item--email\">\n    <a\n      class=\"addthis_button_email socialLinks__link socialLinks--link icon icon--email\"\n      title=\"{{{capitalize service}}}\"\n      href=\"mailto:?subject={{getVar 'encodedTitle'}}&amp;body={{getVar 'encodedUrl'}}\"\n      target=\"_self\"\n      rel=\"noopener\"\n      {{{annotation}}}\n    >\n      <i class=\"icon icon-socialLinks\">\n        <svg>\n          <use xlink:href=\"#icon-envelope\"></use>\n        </svg>\n      </i>\n      <span class=\"socialLinks-title\">Email</span>\n    </a>\n  </li>\n</ul>\n\n<script\n  type=\"text/javascript\"\n  defer\n  src=\"//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-4e94ed470ee51e32\"\n></script>\n<script>\n  window.addEventListener(\"DOMContentLoaded\", function () {\n    if (typeof addthis === \"object\") {\n      addthis.toolbox(\".addthis_toolbox\");\n    }\n  });\n</script>\n"}
